{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "money-sprouts": {
            "projectType": "application",
            "root": "",
            "sourceRoot": "assets/client",
            "prefix": "money-sprouts",
            "architect": {
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "assets/client/**/*.ts",
                            "assets/client/**/*.html"
                        ]
                    }
                }
            }
        }
    }
}
